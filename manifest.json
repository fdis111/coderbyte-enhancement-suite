{
  "manifest_version": 3,
  "name": "Coderbyte Enhancement Suite",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["https://coderbyte.com/dashboard/*"],
      "js": ["jquery-3.5.1.min.js", "content.js"]
    }
  ],
  "action": {
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["downloads"]
}
